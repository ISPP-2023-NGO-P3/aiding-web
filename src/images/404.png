<script>
    // When element disappears show counter banner
    window.onscroll = function () {
        var element = document.getElementById("js-show-modal-when-dissapear");
        var counterBanner = document.getElementById("js-counter-banner");
        var elementHeight = element.offsetHeight || element.clientHeight;
        var elementDistance = element.getBoundingClientRect().top;
        if (elementDistance <= elementHeight * -1) {
            counterBanner.classList.add("c-counters__banner--active");
        } else {
            counterBanner.classList.remove("c-counters__banner--active");
        }
    }

    jQuery(document).ready(function ($) {
        var productTitle = $('#product-banner-title');

        truncateTitle();

        $(window).on('resize', truncateTitle);

        /**
         * Truncates the product title for mobile
         *
         * @returns {void}
         */
        function truncateTitle() {
            if ($(window).width() < 600) {
                if (productTitle.text().length > 25) {
                    productTitle.text(productTitle.text().substr(0, 25) + '\u2026');
                }
            } else {
                productTitle.text('Illustration Concept of 404 Error Page');
            }
        }

        $('#buy-on-scroll .single_add_to_cart_button, #buy-on-scroll .fa-download').click(function (e) {
