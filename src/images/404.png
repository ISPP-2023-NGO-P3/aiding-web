        $('body').on('click', '*[data-publish-post-modal]', function(e) {
            e.preventDefault();

            var modalType = $(this).data('publish-post-modal');
            var $modal = $('.c-modal--feed[data-post-type="'+modalType+'"]');

            $modal.find('.c-modal-inner').show();
            $modal.find('.c-modal-result').hide();
            $modal.modal('show');
        });

        // Publish post of type project/simple
        $('.c-modal .publish-post').click(function(e) {
            e.preventDefault();
            var $cta = $(this);
            $cta.attr('disabled', true);

            var $modal = $cta.closest('.c-modal');
            var $form = $modal.find('form');
            var $alert = $modal.find('.c-modal__header-alert--invalid');
            var $spinner = $cta.find('.fa-spin');

            var description = $form.find('textarea[name="description"]').val();
            if (description === '' || typeof description === 'undefined') {
                $alert.html('Please leave a comment.');
                $alert.show();
                $cta.attr('disabled', false);

                return;
            }

            if (description.length > 5000) {
                $alert.html('Please make sure your text has less than 5000 characters.');
                $alert.show();
                $cta.attr('disabled', false);

                return;
            }

            // If it is project post type, check that at least 1 image
            // has been uploaded
            var postType = $form.find('input[name="post_type"]').val();
            if (postType === 'project' || postType === 'cfclassproject' || postType === 'cfproductproject') {
                var images = $form.find('input[name="images_list[]"]').val();
                if (images === '' || typeof images === 'undefined') {
                    $alert.html('Please add some pictures.');
                    $alert.show();
                    $cta.attr('disabled', false);

                    return;
                }
            }

            $spinner.show();
            $alert.hide();

            $.ajax({
                url: 'https://www.creativefabrica.com/wp-admin/admin-ajax.php',
                type: 'POST',
                data: $form.serialize() + '&action=social_publish_post&security=ff18d549d3',
                dataType: 'json',
                success: function (data) {
                    if (data.status === 1) {
                        cfTrackEvent('Posted on social', data.socialInfo, 'action');

                        var url = new URL(window.location.href);
                        url.searchParams.delete('showProjectModal');

                        if (postType === 'cfclassproject' || postType === 'cfproductproject') {

                            window.open(data.postUrl);
                        }
                        location.href = url.toString();
                    } else {
                        $alert.html(data.message);
                        $alert.show();
                        $spinner.hide();
                        $cta.attr('disabled', false);
                    }
                },
                error: function (error) {
                    $cta.attr('disabled', false);
                    $spinner.hide();
                }
            });

        });

        // Trigger file uploader
        $('.c-modal .add-picture').click(function(e) {
            e.preventDefault();
            $('.fileuploader-thumbnails-input-inner').click();
        });

        const urlParams = new URLSearchParams(window.location.search);
        const showProjectModal = urlParams.get('showProjectModal');

        if (showProjectModal && showProjectModal == '1') {
            // if the product has been purchased, this will exist
            setTimeout(() => $('button[data-publish-post-modal=cfproductproject]').click(), 500);
            // if the product has not been purchased, this will exist
            setTimeout(() => $('button#promo-upsell-popup').click(), 500);
            // or this one will exist ...
            setTimeout(() => $('button#button-no-review-project').click(), 500);
            // this one will exist on class pages
            setTimeout(() => $('button[data-publish-post-modal=cfclassproject]').click(), 500);
        }

    });
</script>

