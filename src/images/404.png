<script>
	Array.prototype.unique = function () {
		return this.filter(function (value, index, self) {
			return self.indexOf(value) === index;
		});
	}
	jQuery(document).ready(function ($) {

	    $("#ajax-report-request").click(function(e) {
			e.preventDefault();

			var currentButton = $(this);
			var currentForm = $(this).closest("form");
			var currentScreen = $(currentForm).closest(".c-modal");
			currentButton.prepend("<i class=\"fa fa-spinner fa-spin u-margin--right-20\"></i>");

			var modalMessage = currentScreen.find(".c-modal__header-alert");
			var formAction = currentButton.data("action");

			var formData = currentForm.serialize();

			formData += "&action=cf_ajax_product_copyright_report&security=875bfd9454";

			$(modalMessage).empty().hide();

			$.ajax({
				type: "POST",
				url: 'https://www.creativefabrica.com/wp-admin/admin-ajax.php?cfsecure',
				dataType: "json",
				data: formData
			}).done(function (result) {
				var messages = {};
				currentButton.find(".fa").remove();

				if (typeof (result.error) === "undefined" && typeof (result.success) === "undefined" && typeof (result.notice) === "undefined") {
				    modalMessage.addClass("c-modal__header-alert--invalid").removeClass("c-modal__header-alert--success");
				    modalMessage.append("<span>Something went wrong. Please try again.</span>");
				    modalMessage.show();
				}
				else {
					if (typeof (result.error) !== "undefined") {
						modalMessage.addClass("c-modal__header-alert--invalid").removeClass("c-modal__header-alert--success");
						messages = result.error;
					}
					else {
						modalMessage.addClass("c-modal__header-alert--success").removeClass("c-modal__header-alert--invalid");
						messages = (typeof (result.notice) === "undefined") ? result.success : result.notice;
						modalMessage.addClass("c-modal__header-alert--success").removeClass("c-modal__header-alert--invalid");
						$(currentScreen).find(".c-headline").html('<i class="c-icon--green fa fa-check-circle u-mr-10" aria-hidden="true"></i> Thank you for your message. Our community team will investigate this as soon as possible and will get back to you as soon as the investigation has been completed.');
						currentForm.hide();
					}
					if (messages.length) {
						var uniqueMessages = messages.unique();
						for (var i = 0; i < uniqueMessages.length; i++) {
							modalMessage.append("<span>" + uniqueMessages[i] + "</span>");
						}
						modalMessage.show();
					}
				}
			});
		});

		$(".form-ajax-report").submit(function (e) {
			e.preventDefault();
		});

	});
</script>

