<script type="text/javascript">
    function handleReviewWrapperReadMore($){
        var reviewContainer = $('.c-product-box--review__text');

        reviewContainer.each(function() {
            var reviewWrapper = $(this).find('.text-wrapper');
            var reviewText = $(this).find('.review-text');

            if(reviewText.height() > reviewWrapper.height()) {
                // Check that the read more option it not already visible for the review container
                let children = $(this).children()[1];

                if(typeof children == 'undefined') {
                    var e = "+ read more";
                    $(this).append('<p class="review-expand">' + e +'</p>');
                    $('.review-expand').html(e);

                    $(this).find('.review-expand').click(function() {
                        $(this).toggleClass('expand-close').prev('.text-wrapper').toggleClass('reveal');
                        if($('.review-expand').html().includes('- read less')){
                            $('.review-expand').html("+ read more");
                        }else {
                            $('.review-expand').html("- read less");
                        }
                    });
                }
            }
        });
    }

