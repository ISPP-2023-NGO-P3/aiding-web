<script type="text/javascript">
    (function () {
        jQuery(document).ready(function($) {

            function makeRequest(action, data, callback) {
                data['action'] = action;
                jQuery.ajax({
                    type: 'POST',
                    url: 'https://www.creativefabrica.com/wp-admin/admin-ajax.php',
                    dataType: 'json',
                    data: data,
                    error: function(data, transport) {
                    },
                    success: function(result) {
                        callback && callback(result);
                    }
                });
            }

            $(".el-container" ).hover(function() {
                $(this).addClass('highlight-green');
                $(this).find('.form-label').addClass('u-green');
            }, function() {
                $(this).removeClass('highlight-green');
                $(this).find('.form-label').removeClass('u-green');
            });

            $(document).on('click', '.multicurrency-cancel', function() {
                $.modal.close();
            })

            $(document).on($.modal.BEFORE_CLOSE, function(event, modal) {
                const goTo = $('#lang-currency-view .multicurrency-cancel').data('go_to');
                if (goTo) {
                    location.href = goTo;
                }
            });

            $(document).on('click', '.multicurrency-save', function() {
                const buttonThis = $(this);
                buttonThis.prop('disabled', true);
                buttonThis.find('.fa-spinner').show();

                const nonce = "af9d7a1bf3";
                const currency = $('#multicurrency-currency').val();
                const lang = $('#multicurrency-language').val();
                const goTo = $('#multicurrency-language').children("option:selected").data('ref');
                const confirmCurrencySwitch = buttonThis.data('confirm_currency_switch') === true;
                const data = {
                    lang : lang,
                    currency: currency,
                    confirmCurrencySwitch: confirmCurrencySwitch ? 1 : 0,
                    security: nonce
                };

                makeRequest('cf_switch_lang_and_currency', data, function(result) {
                    if (typeof ga !== 'undefined' && (result.data['isCurrencySwitch'] || result.data['isLanguageSwitch'])) {
                        ga('send', {
                            "hitType": "event",
                            "eventCategory": "Header",
                            "eventAction": "Language/currency changed",
                            "eventLabel": "en->" + data.lang + ",EUR->" + data.currency
                        });
                    }

                    
                    if (Object.hasOwn(result['data'], 'view')) {
                        var $view = $(result['data']['view']);
                        // If currency switch was not confirmed but
                        // language was still switched, keep track of the lang url
                        // to go to if cancelling the currency switch
                        if (result.data['isLanguageSwitch']) {
                            $view.find('.multicurrency-cancel').data('go_to', goTo);
                        }
                        $('#lang-currency-view').append($view);
                        $('#lang-currency-switcher').hide();
                        return;
                    }

                    location.href = goTo;
                });
            })
         });
    })();
</script>

<script>
jQuery(document).ready(function($) {
    $(document).on('click', '#language-switcher', function() {
        restoreMulticurrencyPopup();
        $('#multicurrency-popup').modal({
            escapeClose: true,
            clickClose: true,
            showClose: true
        });
    })
    //move language toggle to mobile menu on sm screens
    $(this).ready(showLanguage);
    $(window).on('resize', showLanguage);

    function showLanguage() {
        if ($(window).width() <= 936) {
            $('#mm-language-mobile').append($("#language-switcher"));
        } else {
            $('#language-desktop').append($("#language-switcher")).addClass('u-inline');
        }
    }

    function restoreMulticurrencyPopup()
    {
        $('#multicurrency-language').val('en');
        $('#multicurrency-currency').val('EUR');
        $('#lang-currency-switcher').show();
        $('#lang-currency-view').html('');
        $('.multicurrency-cancel').removeData('go_to');
        $('.multicurrency-save').removeProp('disabled');
        $('.multicurrency-save .fa-spinner').hide();
    }
});
</script>
