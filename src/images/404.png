<script type="text/javascript">
    if (window.innerWidth < 576) {
        document.getElementById('zdbutton--content').innerText = '';
        document.getElementById('zdbutton--content').classList.remove('u-ml-5');
        document.getElementById('zdbutton--svg').setAttribute('width', '20');
    } else {
        document.getElementById('zdbutton--content').innerText = 'Support';
        document.getElementById('zdbutton--content').classList.add('u-ml-5');
        document.getElementById('zdbutton--svg').setAttribute('width', '25');
    }

    function loadAndOpenZendeskChat() {
        var button = document.getElementById('zdbutton');

        button.innerHTML = '<i class="fa fa-spinner fa-spin">';

        loadZendeskScript(function () {
            button.remove();

            zE(function () {
                zE.activate();
            });

            window.zESettings = {
                webWidget: {
                    offset: {
                        horizontal: '0px',
                        vertical: '0px',
                        mobile: {
                            horizontal: '15px',
                            vertical: '110px'
                        }
                    },
                    chat: {
                        connectOnPageLoad: false
                    }
                }
            };
        });
    }

    function createZendeskScript() {
        var script = document.createElement('script');

        script.type = 'text/javascript';
        script.src = 'https://static.zdassets.com/ekr/snippet.js?key=ca315e16-f0e4-42be-822f-8c36bcc9bd26';
        script.setAttribute('id', 'ze-snippet');

        return script;
    }

    function loadZendeskScript(callback) {
        document.getElementsByTagName('body')[0]
            .appendChild(createZendeskScript());

        window.zdonload = setInterval(function () {
            if (typeof zE !== 'undefined' && typeof zE.activate !== 'undefined') {
                clearInterval(window.zdonload);
                callback();
            }
        }, 50)
    }
</script>


