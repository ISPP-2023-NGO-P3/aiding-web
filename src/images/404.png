            $.ajax({
                type: "POST",
                url: 'https://www.creativefabrica.com/wp-admin/admin-ajax.php',
                dataType: 'json',
                data: {
                    "perPage"  : '8',
                    "currentPage" : currentPage,
                    "itemId" : 8662746,
                    "itemAuthorId": 172423,
                    "reviewType": 1,
                    "action" : 'readMoreReviews',
                    "security" : 'dce9d5e633'
                },
                error: function(data, transport) {

                },
                success: function(result) {
                    if(result.status){
                        $('.review-list').append(result.message);
                        $spinner.remove();

                        //read more for review boxes of newly added reviews/comments
                        handleReviewWrapperReadMore($);

                        if(result.buttonRemove){
                            $('.show-more-vertical-line').hide();
                            $('.show-more-div').hide();
                        }
                    }
                }
            });
        });
    });
