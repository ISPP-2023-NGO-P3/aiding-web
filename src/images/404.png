                            return false;
                        } else {
                            $subMenu.before('<p class="parent-title">' + oldText + '</p>');

                            return false;
                        }
                    });
                }
            });

            api.bind( "close:after", function() {
                api.closeAllPanels();
            });
        }

		jQuery('.mm-menu .primary-navigation .mm-navbar').click(function() {
			api.close();
		});

		var w = jQuery('#mm-site-navigation').outerWidth(true);
