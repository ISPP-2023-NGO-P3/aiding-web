                jQuery('#menu-second').show();
            }
        }

		nav_mobile();
		var api = jQuery(".main-navigation").data( "mmenu" );

        if (typeof api !== 'undefined') {
            api.bind( 'openPanel:before', function($panel) {
                var navbar = $panel.find('.mm-navbar__title');
                var oldText = navbar.html().trim();

                navbar.html('Main Menu');

                if ($panel.find('.parent-title').length === 0) {
                    var $subMenu =  $panel.find('.sub-menu');

                    $subMenu.find('.menu-item a').each(function (i, item) {
                        if ($(item).attr('href').includes('/ideas/')) {
