            .remove();

            });

    
	function moveMenuIconsToNav() {
        var mainMenu = jQuery('#menu-main');
        var iconExist = mainMenu.find('.micons');

		if (iconExist.length === 0) {
            var itemIcons = jQuery('.menu-item-icon').get().reverse();

                            jQuery(itemIcons).each(function (index) {
                    var li = jQuery('<li class="show-on-mobile menu-item micons"></li>');
                    li.append(jQuery(itemIcons[index]).clone());
                    jQuery('#menu-main').prepend(li);
                });
            		}
	}

    /**
     * This function moves the "secondary" menu items, such as Freebies
     * and Gifts, to the main mobile menu
     *
     * @returns {void}
     */
    function moveSecondaryMenuItemsToPrimary() {
        var $secondaryMenuItems = jQuery('#menu-second > li');
        var $primaryMenu = jQuery('#menu-main');

        $secondaryMenuItems.each(function () {
            // There are additional menu items that are not shown
            // that we should not add to the mobile menu;
            // they all have an ID, so let's return
            if (jQuery(this).attr('id')) {
                return;
            }

            // We clone the entire <li> element
            var $element = jQuery(this).clone();

            // We add the classes necessary for the mobile menu
            $element
                .addClass('show-on-mobile menu-item micons')
                .find('a > span')
                .css({
                    'font-size': '13px',
                    'font-weight': 'normal'
                });

            // Go through each child to find "Gifts"
            $element.each(function() {
                // Get the text that is displayed
                var $el = jQuery(this).find('a > span');

                // If the text is "gifts" we want to hide
                // the gift icon in the mobile menu
                if ($el.text().trim().toLowerCase() === 'gifts') {
                    $el.html('Gifts');
                }
            });

            $primaryMenu.append($element);
        });
    }

	function nav_mobile() {
		moveMenuIconsToNav();
        moveSecondaryMenuItemsToPrimary();

		jQuery(".main-navigation").mmenu({}, {
    		"clone" : true
        });
	}

</script>

<script>

	jQuery(document).ready(function($) {

        // Get a usable value for the event label from the url
        // Most links use a path with a trailing slash eg /path-text/
        // But we also need to handle some affiliate links that
        // use a query string eg ?tab=path-text
        function processUrl(url) {
            var path;

            // Check if we're dealing with an affiliate link that uses a query string
            if (~url.indexOf('?tab=')) {
                // split the url on the query string
                path = url.split('?tab=');
                // get the query string value
                path = path[path.length - 1];
                // trim the trailing slash
                path = path.replace(/\/$/, "")
            } else {
                // split the url on the forward slashes
                path = url.split('/')
                // get the last section of the url
                path = path[path.length - 2];
            }

            // remove any dashes
            path = path.replace(/-/g, ' ');
            // capitalise the first letter of the path
            path = path.replace(/([a-z])/, function (match, value) {
                         return value.toUpperCase();
                     });

            return path;
        }

        function trackGAEventHeader(
            eventAction,
            eventLabel
        ) {
           if (typeof ga !== 'undefined') {
                ga('send', {
                    "hitType": "event",
                    "eventCategory": "Header",
                    "eventAction": eventAction,
                    "eventLabel": eventLabel,
                    "transport": "beacon"
                });
            }
        }

        // Logo clicked
        $('#masthead').on('click', '.site-title a', function() {
            trackGAEventHeader(
                'Logo clicked',
                'Logo'
            )
        });

        // Main navigation
        $('#menu-main').on('click', 'a', function () {
            var $this = $(this);
            var url = $this.attr('href');
            var path = processUrl(url);
            var isSecondaryLink = $this.parents('.sub-menu').length;
            var $parent;
            var parentPath;

            // Standard dropdown links
            if (
                isSecondaryLink &&
                $this.parents('.menu-item-has-children').length
            ) {
                $parent = $this.closest('.menu-item-has-children').find('a:first');
                parentPath = processUrl($parent.attr('href'));

                // discounted links eg /promotions/font/
                if (~url.indexOf('/promotions/')) {
                    path = parentPath + ', Discounted ' + path;
                // Teacher club card
                } else if (~url.indexOf('/teachers/')) {
                    path = 'Teachers, Teacher Club Card';
                // Teacher club card
                } else if (~url.indexOf('/teaching-materials/')) {
                    path = 'Teachers, Teaching Resources';
                // browse all links eg /fonts/
                } else if (parentPath === path) {
                    path = parentPath + ', Browse all';
                } else {
                    path = parentPath + ', ' + path;
                }

                trackGAEventHeader(
                    'Secondary menu item clicked',
                    path
                )
            // Idea/light bulb icon dropdown links
            } else if (isSecondaryLink) {
                path = 'Ideas, ' + path;

                trackGAEventHeader(
                    'Secondary menu item clicked',
                    path
                )
            // Idea/light bulb icon top level link
            } else if ($this.is('#ideas')) {
                trackGAEventHeader(
                    'Menu item clicked',
                    'Ideas'
                )
            // Teachers top level link
            } else if (~url.indexOf('/teaching-materials/')) {
                trackGAEventHeader(
                    'Menu item clicked',
                    'Teachers'
                )
            // Standard top level link
            } else {
                trackGAEventHeader(
                    'Menu item clicked',
                    path
                )
            }
        });

        // Account items
        $('.menu-icons').on('click', 'a', function () {
            var $this = $(this);
            var url = $this.attr('href');
            var path = processUrl(url);
            var isSecondaryLink = $this.parents('.menu-preview').length;
            var eventAction;
            var eventLabel;

            // View my profile link
            if (
                isSecondaryLink &&
                ~url.indexOf('/crafter/')
            ) {
                eventAction = 'Account menu item clicked';
                eventLabel = 'My Profile, View my profile';
            // My store dropdown
            } else if (
               isSecondaryLink &&
               $this.parents('#menu-item-icon-my-store').length
            ) {
                eventAction = 'My store menu item clicked';
                // My store dropdown my store link
                if (~url.indexOf('/designer/local_designer/')) {
                    eventLabel = 'My store, My store';
                } else {
                    eventLabel = 'My store, ' + path;
                }
            // My profile dropdown
            } else if (isSecondaryLink) {
                eventAction = 'Account menu item clicked';
                eventLabel = 'My Profile, ' + path;
            // My store link
            } else if ($this.parent().is('#menu-item-icon-my-store')) {
                eventAction = 'My store item clicked';
                eventLabel = 'My store';
            // Open store link
            } else if (~url.indexOf('/open-store')) {
                eventAction = 'Account item clicked';
                eventLabel = 'Open store';
            // My profile link
            } else if (~url.indexOf('/masterfeed')) {
                eventAction = 'Account item clicked';
                eventLabel = 'My profile';
            // Standard top level account links
            } else {
                eventAction = 'Account item clicked';
                eventLabel = path;
            }

            trackGAEventHeader(
                eventAction,
                eventLabel
            )
        });

        // Promotion items
        $('#menu-second').on('click', 'a', function () {
            var $this = $(this);
            var url = $this.attr('href');
            var path = processUrl(url);
            var isSecondaryLink = $this.parents('.sub-menu').length;
            var $parent;
            var parentPath;

            // Dropdown links
            if (isSecondaryLink) {
                $parent = $this.closest('.menu-item-has-children').find('a:first');
                parentPath = processUrl($parent.attr('href'));
                path = parentPath + ', ' + path;
            }

            trackGAEventHeader(
                'Promotion item clicked',
                path
            )
        });

        // Mobile top level
        $('#mm-menu-main').on('click', 'a', function () {
            var $this = $(this);
            var url;
            var eventAction;
            var eventLabel;

            // My Account link
            if ($this.attr('href') === '#mm-2') {
                trackGAEventHeader(
                    'Account item clicked',
                    'Mobile menu, My Account',
                    true
                )
                return;
            // Standard links
            } else if ($this.is('.mm-listitem__text')) {
                url = $this.attr('href');
            // Dynamic urls (open sub-menu)
            } else {
                url = $this.parent().find('.mm-listitem__text').attr('href');
            }

            // Teachers top level mobile menu link
            if (~url.indexOf('/teaching-materials/')) {
                eventLabel = 'Mobile menu, Teachers';
            } else {
                eventLabel = 'Mobile menu, ' + processUrl(url);
            }

            if ($this.parent().is('.secondary-menu-item')) {
                eventAction = 'Promotion item clicked';
            } else if ($this.is('.menu-item-icon')) {
                eventAction = 'Account item clicked';
            } else {
                eventAction = 'Main menu item clicked';
            }

            trackGAEventHeader(
                eventAction,
                eventLabel
            )
        });

        // Mobile sub level
        $('.sub-menu').on('click', '.mm-listitem__text', function () {
            var $this = $(this);
            var url = $this.attr('href');
            var $parentID = $this.closest('.mm-panel_opened').attr('id');
            var $parent = $('#mm-menu-main').find('[aria-owns="' + $parentID + '"]').parent();
            var path = processUrl(url);
            var parentUrl = $parent.find('.mm-listitem__text:first').attr('href');
            var parentPath;
            var eventAction;
            var eventLabel;


            if(parentUrl.length) {
                eventAction = 'Main menu item clicked';
                parentPath = processUrl(parentUrl);

                // discounted links eg /promotions/font/
                if (~url.indexOf('/promotions/')) {
                    eventLabel = 'Mobile menu, ' + parentPath + ', Discounted ' + path;
                // Teacher club card
                } else if (~url.indexOf('/teachers/')) {
                    eventLabel = 'Mobile menu, Teachers, Teacher Club Card';
                // Teacher club card
                } else if (~url.indexOf('/teaching-materials/')) {
                    eventLabel = 'Mobile menu, Teachers, Teaching Resources';
                // browse all links eg /fonts/
                } else if (parentPath === path) {
                    eventLabel = 'Mobile menu, ' + parentPath + ', Browse all';
                } else {
                    eventLabel = 'Mobile menu, ' + parentPath + ', ' + path;
                }
            // My account, affiliate and designer links
            } else {
                eventAction = 'Account item clicked';
                // logout link
                if (~url.indexOf('/customer-logout/')) {
                    eventLabel = 'Mobile menu, My Account, Log out';
                // designer links eg /dashboard/product-add/font
                } else if (~url.indexOf('/dashboard/product-add/')) {
                    eventLabel = 'Mobile menu, My Account, Add ' + path;
                } else {
                    eventLabel = 'Mobile menu, My Account, ' + path
                }
            }

            trackGAEventHeader(
                eventAction,
                eventLabel
            )
        });

        // Mobile menu close button
        $('#mm-site-navigation').on('click', '.primary-navigation .mm-navbar', function() {
            trackGAEventHeader(
                'Mobile menu button clicked',
                'Mobile: close menu'
            )
        });

        // Search bar tracking

        // Search bar entered text
        $('#woocommerce-product-search-field').one('keyup', function(e) {
            if (this.value.length > 0) {
                trackGAEventHeader(
                    'Search bar used',
                    'Entered text'
                );
            }
        });

        // Autocomplete result clicked
        $('.aa-dropdown-menu').on('click', '.aa-suggestion', function() {
            trackGAEventHeader(
                'Search bar used',
                'Autocomplete result'
            );
        });

        $('.woocommerce-product-search').on('click', 'input[type=submit]', function(e) {
            // Submit button clicked
            if (e.detail) {
                trackGAEventHeader(
                    'Search bar used',
                    'Search button'
                )
            // Form submitted via enter key press
            } else {
                trackGAEventHeader(
                    'Search bar used',
                    'Enter key'
                );
            }

        });
    });
</script>


