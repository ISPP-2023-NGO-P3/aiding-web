<script type="text/javascript">
	Array.prototype.unique = function () {
		return this.filter(function (value, index, self) {
			return self.indexOf(value) === index;
		});
	}

	function TogglePassword(triggerID, inputID) {
		triggerID.addEventListener("click", function (e) {
			if (inputID.type === "password") {
				inputID.type = "text";
			}
			else {
				inputID.type = "password";
			}
		});
	}
</script>
<script>
    var registrationRedirectExperimentRedirectUrl = 'https://www.creativefabrica.com/special-signup-offer?redirect=';
    var registrationRedirectExperiment = function (redirect) {
        return redirect;
    };

	function setSegmentDataInModals($, segmentLocation) {
		$("#modal-register").find('input[name="amplitude_location"]').attr('value', segmentLocation);
		$("#modal-register").find('div[data-step=credentials]').find('a[rel=nofollow]').each(function() {
			var href = new URL($(this).attr('href'));
			href.searchParams.set('location', encodeURIComponent(segmentLocation));
			$(this).attr('href', href.toString());
		});
	}

	jQuery(document).ready(function ($) {
		$('body').on("click", ".show-modal-account", function(e) {
			// fallback for any buttons that don't have an explicit
			// tracking location set
			var segmentLocation = $(this).attr('data-amplitude-location') || 'rest';
			e.preventDefault();
			$(".c-modal-account").hide();
			$("#modal-login").show();
			$("#modal-account").modal();
			cfTrackModalOpened('login', segmentLocation);
			setSegmentDataInModals($, segmentLocation);
		});

		$('body').on("click", ".show-modal-signup", function(e) {
			e.preventDefault();

			// fallback for any buttons that don't have an explicit
			// tracking location set
			var segmentLocation = $(this).attr('data-amplitude-location') || 'rest';
			cfTrackModalOpened('registration', segmentLocation);
			setSegmentDataInModals($, segmentLocation);

			$(".c-modal-account").hide();
			$("#modal-register").show();
			$("#modal-account").modal();
		});

		$("#modal-account").on("click", ".modal-switch-view", function (e) {
			e.preventDefault();
			var new_view = $(this).data("view");
			if ($("#" + new_view).length > 0) {
				$(".c-modal-account").hide();
				$("#" + new_view).show();

				switch(new_view) {
					case 'modal-register':
						cfTrackModalOpened('registration', 'login modal');
						break;
					case 'modal-login':
						cfTrackModalOpened('login', 'registration modal');
						break;
				}
			}
		});

        // Switches step within a screen. E.g. From the OTP step
        // back to the credential form on the register modal -> Change email link
		$("#modal-account").on("click", "[data-switch-step]", function (e) {
			e.preventDefault();

			var $currentScreen = $(this).closest(".c-modal");

			var oldStep = $currentScreen.data("active-step");
			var newStep = $(this).data("switch-step");

			$currentScreen.find('[data-step="'+oldStep+'"]').hide();
			$currentScreen.find('[data-step="'+newStep+'"]').show();
			$currentScreen.find('[data-step="'+oldStep+'"] input').prop('disabled', true);
			$currentScreen.find('[data-step="'+newStep+'"] input').prop('disabled', false);

			$currentScreen.data("active-step", newStep);
		});

        // Resends OTP by submitting only the input fields within the "credentials" step
        // Since this is triggered from the OTP step, the field in it (OTP code) is
        // disabled so it is not sent through. It is enabled again when the request ends
		$("#modal-account").on("click", "[data-resend-otp]", function (e) {
			e.preventDefault();

			var $currentScreen = $(this).closest(".c-modal");
			$currentScreen.find('button[type="submit"]').prop('disabled', true);
			$currentScreen.find('[data-step="otp"] input').prop('disabled', true);
			$currentScreen.find('.resend-otp-success').hide();

			var action = $currentScreen.find('button[type="submit"]').data('action');
			submitForm(action, function() {
				$currentScreen.find('button[type="submit"]').prop('disabled', false);
				$currentScreen.find('[data-step="otp"] input').prop('disabled', false);
				$currentScreen.find('.resend-otp-success').show();
			});
		});

		$(".form-ajax").submit(function (e) {
			e.preventDefault();

			var currentButton = $(this).find(".c-button-account-ajax");
			var action = currentButton.data("action");
			var spinner = currentButton.find(".fa-spinner");
			var currentScreen = $(this).closest(".c-modal");
			var modalMessage = currentScreen.find(".c-modal__header-alert");

			$(modalMessage).empty().hide();
            currentButton.prop('disabled', true);
			spinner.show();

			submitForm(action);

			//if (!isIE10prior()) {
			//	generate_recaptcha_token(action, submitForm);
			//} else {
			//	submitForm(action);
			//}

		});


		function submitForm(action, callback)
		{
			if (arguments.length === 1) {
				callback = false;
			}

			var currentForm = $('form[data-action="'+action+'"]')
            var $currentButton = currentForm.find('.c-button-account-ajax');
			var $spinner = currentForm.find(".c-button-account-ajax .fa-spinner");
			var currentScreen = currentForm.closest(".c-modal");
			var $modalMessage = currentScreen.find(".c-modal__header-alert");
			var formData = currentForm.serialize();

			$.ajax({
				type: "POST",
				url: "https://www.creativefabrica.com/cfsecure/" + action + "/",
				dataType: "json",
				data: formData
			})
            .error(function (xhr) {
                var messages = [];
                var errorTitle = '<strong>Error: </strong>';
                var response = JSON.parse(xhr.responseText);

                $modalMessage
                    .addClass("c-modal__header-alert--invalid")
                    .removeClass("c-modal__header-alert--success");

                response.errors.forEach(function (error) {
                    var message = error.message
                        ? error.message
                        : error.code;

                    if (error.code === 'facebook_email_already_exists') {
                        errorTitle = '<strong class="fbaccount">Error: </strong>';
                    }

                    if (error.code === 'google_email_already_exists') {
                        errorTitle = '<strong class="fbaccount">Error: </strong>';
                    }

                    if (error.code === 'invalid_otp') {
                        message += ' <a href="#" data-resend-otp="">Resend</a>';
                    }

                    messages.push(message);
                });

                if (messages.length) {
                    var uniqueMessages = messages.unique();

                    for (var i = 0; i < uniqueMessages.length; i++) {
                        $modalMessage.append(errorTitle + "<span>" + uniqueMessages[i] + "</span>");
                    }

                    $modalMessage.show();

                    if ($(".fbaccount").length) {
                        if (action === "lostpassword") {
                            $(".fb-login").show();
                        }
                    }
                }

                if ($currentButton.length) {
                    $currentButton.prop('disabled', false);
                }

                $spinner.hide();
            })
            .success(function (response) {
                if (typeof (response.challenge) !== "undefined") {
                    $currentButton.prop('disabled', false);

                    if (response.challenge === 'otp') {
                        currentScreen.data('active-step', 'otp');
                        currentScreen.find('[data-step="credentials"]').hide();
                        currentScreen.find('[data-step="otp"]').show();
                        currentScreen.find('[data-otp-text]').html(response.text);
                        currentScreen.find('.resend-otp-success').hide();
                        currentForm.find('input[name=otp]').prop('disabled', false);
                        currentForm.find('input[name=otp]').val('');
                        currentForm.find('input[name=otp]').focus();

                        // added so that the 'Registration' form can turn
                        // into a 'Log In' form
                        if (response.cta) {
                            currentForm.find('button[type=submit] span').text(response.cta);
                        }
                        if (response.context && response.context == 'login') {
                            currentForm.find('p[id=register-login-cta]').hide();
                        }

                                            } else if (response.challenge === '2fa') {
                        currentForm.find('input[name=code]').prop('disabled', false);
                        currentForm.find('.code-wrapper').show();
                        currentForm.find('input[name=code]').focus();
                    }
                } else if (typeof (response.redirect) !== "undefined") {
                    if (response.redirect === "refresh") {
                        location.reload();
                    } else {
                        if (action === 'register') {
                            location.href = registrationRedirectExperiment(response.redirect);
                        } else {
                            location.href = response.redirect;
                        }
                    }

                    return;
                }

                if (action === "lostpassword") {
                    $modalMessage
                        .addClass("c-modal__header-alert--success")
                        .removeClass("c-modal__header-alert--invalid");

                    $(currentScreen)
                        .find(".c-headline")
                        .html('<i class="c-icon--green fa fa-check-circle u-mr-10" aria-hidden="true"></i> Password reset email has been sent.');
                    currentForm.hide();

                    $modalMessage.append(response.message);
                    $modalMessage.show();
                }

                callback && callback();
            })
            .done(function () {
                $spinner.hide();
            });
		}
	});
</script>
